# Iceberg Test Dashboard Configuration
# Copy this file to .env and fill in the values

# =============================================================================
# API Configuration
# =============================================================================
ICEBERG_API_URL=https://api.botbro.trade

# Dashboard port (default: 8509 per Requirement 1.4)
DASHBOARD_PORT=8509

# =============================================================================
# Google OAuth Configuration
# =============================================================================
# Get this from Google Cloud Console -> APIs & Services -> Credentials
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# OAuth callback URI (per Requirement 3.3)
GOOGLE_CALLBACK_URI=https://botbro.ronykax.xyz/api/auth/callback/google

# =============================================================================
# Optional: Pre-configured JWT token for testing
# =============================================================================
# Get this from browser dev tools after logging in to indicator.ronykax.xyz
ICEBERG_JWT_TOKEN=

# =============================================================================
# Debug Settings
# =============================================================================
DEBUG=false
